<?xml version="1.0" encoding="UTF-8"?>
<suite name="TestNG Suite" verbose="4">
    <!--
  
  
  Sample for test ng
  
    -->
    <test name="Non BaseTest Tests" parallel="classes" thread-count="1">
        <classes>
            <class name="com.dhenton9000.selenium.testng.TestNgSeleniumTests"/>
            <class name="com.dhenton9000.testng.MavenPropertiesTest"/>
        </classes>
    </test>
    <test name="BaseTest Tests TestNg" parallel="classes" thread-count="1">
        <classes>
            <class name="com.dhenton9000.selenium.generic.nytimes.NYTimesTest"/>
        </classes>
        <packages>
            <package name="com.dhenton9000.selenium.sandbox.jquery"/>
        </packages>
    </test>
    <test junit="true" name="BaseTest Tests Junit" parallel="classes" thread-count="1">
        <classes>
            <class name="com.dhenton9000.selenium.generic.GenericSeleniumTests"/>
            <class name="com.dhenton9000.phantom.js.PhantomJSTest"/>
        </classes>
        
    </test>
</suite>
